<div class="container mx-auto">
  <!-- component -->
  <!-- Create By Joker Banny -->
  <div
    class="min-h-screen bg-no-repeat bg-cover bg-center"
    style="background-image: url('<%=  data.pre_image%>')"
  >
    <div class="flex justify-end">
      <div class="bg-white min-h-screen w-1/2 flex justify-center items-center">
        <div>
          <form action="/admin/add-prescription-cart" method="post">
            <div>
              <h1 class="text-2xl font-bold">Add To Cart</h1>
            </div>
            <div class="mt-5">
              <label class="block text-md mb-2" for="userid">User Id</label>
              <input
                class="px-4 w-full border-2 py-2 rounded-md text-sm outline-none"
                type="number"
                name="user_id"
                value="<%= data.user_id%>"
                readonly
              />
            </div>
            <div class="my-3">
              <label class="block text-md mb-2" for="email">Product Id</label>
              <input
                class="px-4 w-full border-2 py-2 rounded-md text-sm outline-none"
                type="number"
                name="product_id"
                placeholder="product_id"
                required
              />
            </div>
    
            <div class="">
              <button
                class="mt-4 mb-3 w-full bg-green-500 hover:bg-green-400 text-white py-2 rounded-md transition duration-100"
              >
                Submit
              </button>
          </form>
          
        </div>
      </div>
    </div>
  </div>
</div>

